@{
}

<h1>User Details</h1>

<div class="card" style="width: 18rem;">
    <div class="card-body">
        <h5 class="card-title">@Model.Name</h5>
        <h6 class="card-subtitle mb-2 text-muted">@Model.Status</h6>
        <p class="card-text">@Model.Email</p>
        <p class="card-text">@Model.Password</p>
        <a asp-controller="User" asp-action="GetAllUsers" type="button" class="btn btn-primary">Back to list</a>

    </div>
</div>
